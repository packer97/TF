            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.regex.Pattern;
import java.util.regex.Matcher;
import java.io.*;

//vars.put(&quot;curThrName&quot;, Thread.currentThread().getName());
String threadNumber = Thread.currentThread().getName();
Pattern pattern = Pattern.compile(&quot;TG \\d+-(\\d+)&quot;);
Matcher matcher = pattern.matcher(threadNumber);
matcher.find();
vars.put(&quot;threadNumber&quot;+matcher.group(1), matcher.group(1));
reader = new BufferedReader(new FileReader(&quot;request&quot;+matcher.group(1)+&quot;.txt&quot;));
String line = reader.readLine();
vars.put(&quot;request&quot;, line);
</stringProp>
